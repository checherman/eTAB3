            var json = {};
            json.html = window.encodeURIComponent($("#paraimprimir").html());
            json.header = $("#header_sala").html();
            json.footer = "eTAB Chiapas";
            json.nombre = $("#header_sala").text();
            if(json){
                $("#htmlhtml").val(json.html);
